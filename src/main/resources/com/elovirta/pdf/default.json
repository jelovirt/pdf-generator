{
  "brand": {
    "toc": {
      "indent": "1em"
    }
  },
  "page": {
    "size": "A4",
    "mirror-margins": true
  },
  "force-page-count": "auto",
  "header": {
    "display-align": "before",
    "end-indent": "10mm",
    "padding-after": "6pt",
    "padding-before": "12pt",
    "start-indent": "10mm",
    "odd": {
      "content": "{chapter}",
      "text-align": "end"
    },
    "even": {
      "content": "{title}",
      "text-align": "start"
    }
  },
  "footer": {
    "end-indent": "10mm",
    "padding-after": "12pt",
    "padding-before": "6pt",
    "start-indent": "10mm",
    "odd": {
      "content": "{folio}",
      "font-weight": "bold",
      "text-align": "end"
    },
    "even": {
      "content": "{folio}",
      "font-weight": "bold",
      "text-align": "start"
    }
  },
  "style": {
    "body": {
      "start-indent": "0pt",
      "space-before": "0.6em",
      "space-after": "0.6em"
    },
    "h1": {
      "title-numbering": true,
      "border-style": "none"
    },
    "h2": {
      "title-numbering": true,
      "border-style": "none"
    },
    "h3": {
      "title-numbering": true
    },
    "h4": {
      "title-numbering": true
    },
    "h5": {
      "title-numbering": true
    },
    "h6": {
      "title-numbering": true
    },
    "chapter": {
      "numbering": "document",
      "title-numbering": true,
      "toc": {
        "1": {
          "start-indent": "0pt",
          "title-numbering": true
        },
        "2": {
          "start-indent": "$brand-toc-indent",
          "title-numbering": true
        },
        "3": {
          "start-indent": "2 * $brand-toc-indent",
          "title-numbering": true
        },
        "4": {
          "start-indent": "3 * $brand-toc-indent",
          "title-numbering": true
        },
        "5": {
          "start-indent": "4 * $brand-toc-indent",
          "title-numbering": true
        },
        "6": {
          "start-indent": "5 * $brand-toc-indent",
          "title-numbering": true
        }
      }
    },
    "topic": {
      "title-numbering": true
    },
    "part": {
      "title-numbering": true,
      "toc": {
        "chapter": {
          "start-indent": "0pt",
          "space-before": "0.6em",
          "font-size": "10pt"
        },
        "1": {
          "start-indent": "0pt",
          "title-numbering": true
        },
        "2": {
          "start-indent": "$brand-toc-indent",
          "title-numbering": true
        },
        "3": {
          "start-indent": "2 * $brand-toc-indent",
          "title-numbering": true
        },
        "4": {
          "start-indent": "3 * $brand-toc-indent",
          "title-numbering": true
        },
        "5": {
          "start-indent": "4 * $brand-toc-indent",
          "title-numbering": true
        },
        "6": {
          "start-indent": "5 * $brand-toc-indent",
          "title-numbering": true
        }
      }
    },
    "appendix": {
      "title-numbering": true
    },
    "toc": {
      "maximum-level": 6,
      "part": {
        "start-indent": "0pt",
        "space-before": "0.6em",
        "space-after": "0.6em"
      },
      "chapter": {
        "start-indent": "0pt",
        "space-before": "0.6em",
        "font-size": "10pt"
      },
      "appendix": {
        "start-indent": "$style-toc-chapter-start-indent",
        "space-before": "$style-toc-chapter-space-before",
        "font-size": "$style-toc-chapter-font-size"
      },
      "1": {
        "start-indent": "0pt",
        "title-numbering": true
      },
      "2": {
        "start-indent": "$brand-toc-indent",
        "title-numbering": true
      },
      "3": {
        "start-indent": "2 * $brand-toc-indent",
        "title-numbering": true
      },
      "4": {
        "start-indent": "3 * $brand-toc-indent",
        "title-numbering": true
      },
      "5": {
        "start-indent": "4 * $brand-toc-indent",
        "title-numbering": true
      },
      "6": {
        "start-indent": "5 * $brand-toc-indent",
        "title-numbering": true
      }
    },
    "index": {
      "space-before": "$style-body-body-space-before",
      "space-after": "$style-body-body-space-after",
      "font-size": "18pt"
    },
    "glossary": {
      "space-before": "$style-body-body-space-before",
      "space-after": "$style-body-body-space-after",
      "font-size": "18pt"
    },
    "b": {
      "font-weight": "bold"
    },
    "i": {
      "font-style": "italic"
    },
    "u": {
      "text-decoration": "underline"
    },
    "tt": {
      "font-family": "monospace"
    },
    "sup": {
      "baseline-shift": "super",
      "font-size": "75%"
    },
    "sub": {
      "baseline-shift": "sub",
      "font-size": "75%"
    },
    "line-through": {
      "text-decoration": "line-through"
    },
    "overline": {
      "text-decoration": "overline"
    },
    "msgph": {
      "font-family": "monospace"
    },
    "msgblock": {
      "extends": "pre"
    },
    "msgnum": {},
    "cmdname": {
      "font-family": "monospace"
    },
    "varname": {
      "font-style": "italic"
    },
    "filepath": {
      "font-family": "monospace"
    },
    "userinput": {
      "font-family": "monospace"
    },
    "systemoutput": {
      "font-family": "monospace"
    },
    "markupname": {
      "font-family": "monospace"
    },
    "xmlelement": {
      "font-family": "monospace"
    },
    "xmlatt": {
      "font-family": "monospace"
    },
    "textentity": {
      "font-family": "monospace"
    },
    "parameterentity": {
      "font-family": "monospace"
    },
    "numcharref": {
      "font-family": "monospace"
    },
    "xmlnsname": {
      "font-family": "monospace"
    },
    "xmlpi": {
      "font-family": "monospace"
    },
    "uicontrol": {
      "font-weight": "bold",
      "line-height": "100%"
    },
    "wintitle": {
      "font-weight": "bold"
    },
    "menucascade": {
      "line-height": "100%"
    },
    "shortcut": {
      "text-decoration": "underline"
    },
    "screen": {
      "extends": "base-font",
      "space-before": "1.2em",
      "space-after": "0.8em",
      "white-space-treatment": "preserve",
      "white-space-collapse": "false",
      "linefeed-treatment": "preserve",
      "wrap-option": "wrap",
      "background-color": "#f0f0f0",
      "font-family": "monospace",
      "line-height": "106%"
    },
    "codeph": {
      "font-family": "monospace"
    },
    "codeblock": {
      "extends": "pre",
      "keep-with-previous.within-page": "always",
      "start-indent": "6pt + from-parent(start-indent)",
      "end-indent": "6pt + from-parent(end-indent)",
      "padding": "6pt",
      "background-color": "#f0f0f0",
      "wrap-option": "wrap",
      "hyphenation-character": "\u25BA"
    },
    "codeblock__top": {
      "leader-pattern": "rule",
      "leader-length": "100%"
    },
    "codeblock__bottom": {
      "leader-pattern": "rule",
      "leader-length": "100%"
    },
    "codeblock.line-number": {
      "font-size": "75%",
      "font-weight": "normal",
      "font-style": "normal",
      "color": "gray",
      "padding-end": "1em"
    },
    "codeblock.whitespace": {
      "color": "#C0C0C0"
    },
    "option": {},
    "var": {
      "font-style": "italic"
    },
    "parmname": {},
    "synph": {},
    "oper": {
      "font-family": "monospace"
    },
    "delim": {
      "font-family": "monospace"
    },
    "sep": {
      "font-family": "monospace"
    },
    "apiname": {
      "font-family": "monospace"
    },
    "parml": {},
    "plentry": {},
    "pt": {
      "font-weight": "bold",
      "text-indent": "0em",
      "end-indent": "24pt"
    },
    "pt__content": {
      "font-weight": "bold"
    },
    "pd": {
      "space-before": "0.3em",
      "space-after": "0.5em",
      "start-indent": "72pt",
      "end-indent": "24pt"
    },
    "synblk": {},
    "synnoteref": {
      "baseline-shift": "super",
      "font-size": "75%"
    },
    "synnote": {
      "baseline-shift": "super",
      "font-size": "75%"
    },
    "syntaxdiagram": {},
    "fragment": {},
    "kwd": {
      "font-family": "monospace"
    },
    "fragref": {
      "font-family": "monospace"
    },
    "fragment.title": {
      "font-weight": "bold"
    },
    "fragment.group": {},
    "syntaxdiagram.group": {}
  }
}
